{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "React and Redux Starter Project",
  "main": "index.js",
  "scripts": {
    "start": "node build/server.js",
    "dev": "babel-node src/server.js",
    "clean": "rimraf build/",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive --watch",
    "build:webpack": "NODE_ENV=production webpack --progress --colors --config webpack/webpack.config.js",
    "build": "npm run clean && gulp && npm run build:webpack",
    "deploy:heroku": "git push heroku master",
    "deploy:github": "git push origin master",
    "deploy": "npm run deploy:github && npm run deploy:heroku"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.4",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.1",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^2.0.0",
    "html-webpack-plugin": "^2.17.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "skin-deep": "^0.16.0",
    "style-loader": "^0.13.1",
    "supertest": "^1.2.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-stream": "^3.2.0"
  }
}
