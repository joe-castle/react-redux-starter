{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "React and Redux Starter Project",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node webpack.devserver.js",
    "clean": "rimraf build/",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' src test",
    "test": "NODE_ENV=test mocha src/**/*.test.js --recursive --compilers js:babel-register",
    "test:watch": "npm test -- --watch",
    "check:src": "npm run lint && npm run test",
    "build": "NODE_ENV=production webpack --progress --colors --config webpack.config.js",
    "predeploy": "npm run clean && npm run check:src && npm run build"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-easy-actions": "^0.4.0",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.2.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.2.1",
    "rupture": "^0.6.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
