{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "React and Redux Starter Project",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --env.dev",
    "clean": "rimraf dist && rimraf src/vendor",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' src",
    "test": "NODE_ENV=test mocha src/**/*.test.js --recursive --compilers js:babel-register",
    "test:watch": "npm test -- --watch",
    "check:src": "npm run lint && npm run test",
    "profile:dev": "webpack --env.dev --profile",
    "profile:prod": "webpack --env.prod --profile",
    "build:app": "webpack --env.prod",
    "build:vendor": "webpack --config webpack.dll.babel.js",
    "build": "npm run clean && npm run check:src && npm run build:vendor && npm run build:app"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-easy-actions": "^0.4.0",
    "shortid": "^2.2.6",
    "styled-components": "^1.4.3",
    "typescript": "^2.5.3",
    "typescript-styled-plugin": "^0.0.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chalk": "^1.1.3",
    "cheerio": "0.22",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "friendly-errors-webpack-plugin": "^1.4.0",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.2.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.3.1",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.6.0",
    "rupture": "^0.6.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.5.0",
    "webpack": "^2.2.1",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.4.1"
  }
}
