{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "React and Redux Starter Project",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --env.dev",
    "clean": "rimraf dist && rimraf src/vendor",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' src",
    "test": "NODE_ENV=test mocha src/**/*.test.js --recursive --compilers js:babel-register",
    "test:watch": "npm test -- --watch",
    "check:src": "npm run lint && npm run test",
    "profile:dev": "webpack --env.dev --profile",
    "profile:prod": "webpack --env.prod --profile",
    "build:app": "webpack --env.prod",
    "build:vendor": "webpack --config webpack.dll.babel.js",
    "build": "npm run clean && npm run check:src && npm run build:vendor && npm run build:app"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.9",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-easy-actions": "^0.4.0",
    "shortid": "^2.2.6",
    "styled-components": "^4.3.2",
    "typescript": "^3.5.3",
    "typescript-styled-plugin": "^0.14.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^0.8.0",
    "chalk": "^2.4.2",
    "cheerio": "0.22",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "extract-text-webpack-plugin": "3.0.2",
    "friendly-errors-webpack-plugin": "^1.4.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.2.0",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.6.0",
    "rupture": "^0.7.1",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.38.0",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^3.7.2"
  }
}
