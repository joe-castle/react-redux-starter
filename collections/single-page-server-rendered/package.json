{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "React and Redux Starter Project",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --env.development",
    "clean": "rimraf build/",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' src test",
    "test": "NODE_ENV=test mocha src/**/*.test.js --recursive --compilers js:babel-register",
    "test:watch": "npm test -- --watch",
    "check:src": "npm run lint && npm run test",
    "build:client-bundle": "webpack --env.production --progress --colors",
    "build": "npm run clean && npm run check:src && npm run build:client-bundle"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "aphrodite": "^1.1.0",
    "classnames": "^2.2.5",
    "eslint-loader": "^1.6.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-easy-actions": "^0.4.0",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.2.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "rupture": "^0.6.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "webpack": "^2.2.1",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.3.0"
  }
}
