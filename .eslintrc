{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "jsx": true,
    "modules": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "no-undef": 0,
    "jsx-quotes": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
  },
  "plugins": ["react"]
}
